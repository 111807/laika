  <tr class="<%= cycle("darkzebra", "lightzebra")%>">
    <td class="lighttext">Member ID</td>
    <td>
      <input id="insurance_provider_patient_member_id" name="insurance_provider_patient[member_id]" size="30" type="text" <% if insurance_provider_patient.member_id %>value="<%= insurance_provider_patient.member_id %>"<% end %> />
    </td>
  </tr>
  <tr class="<%= cycle("darkzebra", "lightzebra")%>">
    <td class="lighttext">Start Coverage Date</td>
    <td>
      <%= calendar_date_select_tag "insurance_provider_patient_start_coverage_date", insurance_provider_patient.start_coverage_date %>
    </td>
  </tr>
  <tr class="<%= cycle("darkzebra", "lightzebra")%>">
    <td class="lighttext">End Coverage Date</td>
    <td>
      <%= calendar_date_select_tag "insurance_provider_patient_end_coverage_date", insurance_provider_patient.end_coverage_date %>
    </td>
  </tr>
  <%= render :partial => '/person_names/edit', :locals => {:person_name => insurance_provider_patient.person_name}%>
  <tr class="<%= cycle("darkzebra", "lightzebra")%>">
    <td class="lighttext">Patient Birth Date</td>
    <td>
      <%= calendar_date_select_tag "insurance_provider_patient_date_of_birth", insurance_provider_patient.date_of_birth %>
    </td>
  </tr>
  <%= render :partial => '/addresses/edit', :locals => {:address => insurance_provider_patient.address}%>
  <%= render :partial => '/telecoms/edit', :locals => {:telecom => insurance_provider_patient.telecom}%>