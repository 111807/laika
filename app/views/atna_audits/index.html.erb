<div class="content">
  <div class="container">
    <h3>Listing message logs</h3>
    <br/>
    <% if flash[:notice] %>
      <div class="notice"><%= flash[:notice] %></div>
    <% end %>
    <% if @atna_audits %>
    <%= will_paginate @atna_audits %>
    <table  id="dashboard">
    <% for message_log in @atna_audits %>
      <tr><th>Created_at</th><td><%=h message_log.timestamp_entry %></td></tr>
      <tr><th>Message</th><td colspan="3"><%=h message_log.message.sub("\r", "\n") %></td></tr>
          </table>
    <table  id="dashboard">
      <% if @values %>
        <% @element_name.each_with_index do |element_name, idx| %>
          <% if idx % 2 == 0 %><th><% else %><tr><th><% end %>
            <%=h element_name %></th><td><%=h @values[idx] %>
          <% if idx % 2 == 0 %></td></tr><% else %></td><% end %>
        <% end %>
      <% end %>
    <% end %>
    </table>
    <%= will_paginate @atna_audits %>
    <% end %>
  </div> <!-- .container -->
</div> <!-- .content -->