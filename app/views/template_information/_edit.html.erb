<% @template_information = template_information %>
<% remote_form_for(:template_information,
                   :url => patient_data_instance_template_information_path(template_information),
                   :update => 'templateInformationContainer', :method => http_method(template_information)) do |f| %>
    <%= f.text_field :name %>
    <br />
    <%= f.submit "Save" %>
<% end %>
