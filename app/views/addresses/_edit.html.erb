  <% fields_for address do |address_form| -%>
  <tr>
    <td class="hitsp_c32_attribute_name">Street Address 1:</td>
    <td class="hitsp_c32_attribute_value">
      <div class="person_editable">
        <%= address_form.text_field :street_address_line_one %>
      </div>
    </td>
    <td class="hitsp_c32_attribute_name">City:</td>
    <td class="hitsp_c32_attribute_value">
      <div class="person_editable">
        <%= address_form.text_field :city %>
      </div>
    </td>
  </tr>
  <tr>
    <td class="hitsp_c32_attribute_name">Street Address 2:</td>
    <td class="hitsp_c32_attribute_value">
      <div class="person_editable">
          <%= address_form.text_field :street_address_line_two %>
      </div>
    </td>
    <td class="hitsp_c32_attribute_name">State:</td>
    <td class="hitsp_c32_attribute_value">
      <div class="person_editable">
<%= address_form.select :state, 
          ["AK",
